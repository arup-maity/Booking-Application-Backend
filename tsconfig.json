{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "strictNullChecks": true,

    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
